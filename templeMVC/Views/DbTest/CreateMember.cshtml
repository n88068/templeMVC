
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>CreateMember</title>
</head>
<body>
    <h2>新增 member</h2>
    <br />
    <form method="post" action="/DbTest/CreateMember">
        <div class="form-group row">
            <label for="inputCharName" class="col-sm-2">編號</label>
            <div class="col-sm-10">
                <input type="text" id="inputCharName" name="id" class="form-control" />
            </div>
        </div>
        <div class="form-group row">
            <label for="inputCardName" class="col-sm-2">名稱</label>
            <div class="col-sm-10">
                <input type="text" id="inputCardName" name="name" class="form-control" />
            </div>
        </div>
        <div class="form-group row">
            <label for="inputCardLevel" class="col-sm-2">電話</label>
            <div class="col-sm-10">
                <input type="number" id="inputCardLevel" name="number" class="form-control" />
            </div>
        </div>
        <input class="btn btn-default" type="submit" value="新增" />
    </form>
</body>
</html>
